(:= add [
  [(+add 0 Y Y)]
  [(-add X Y Z) (+add (s X) Y (s Z))]])

(:= tested (exec (union #add @[(-add 0 0 R) R])))
(== tested 0)

(:= tested (exec (union #add @[(-add (s 0) 0 R) R])))
(== tested (s 0))

(:= tested (exec (union #add @[(-add 0 (s 0) R) R])))
(== tested (s 0))

(:= tested (exec (union #add @[(-add <s s 0> <s s 0> R) R])))
(== tested <s s s s 0>)

(:= tested (exec (union #add @[(-add <s s 0> R <s s 0>) R])))
(== tested 0)

(:= tested (exec (union #add @[(-add <s s 0> R <s s s 0>) R])))
(== tested <s 0>)

(:= tested (exec (union #add @[(-add <s s 0> R <s s s s 0>) R])))
(== tested <s s 0>)
