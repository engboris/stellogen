(:= g {
  [+test1 1]
  [+test2 {
    [+test21 2]
    [+test22 {
      [+test3 3]}]}]})

(:= x <eval interact { #g @[-test1] }>)
(== #x 1)

(:= x <eval interact { #g @[-test2] }>)
(:= y <eval interact { #x @[-test21] }>)
(== #y 2)

(:= x <eval interact { #g @[-test2] }>)
(:= y <eval interact { #x @[-test22] }>)
(:= z <eval interact { #y @[-test3] }>)
(== #z 3)
