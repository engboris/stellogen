(def g {
  [(+field test1) 1]
  [(+field test2) {
    [(+field test21) 2]
    [(+field test22) {
      [(+field test3) 3]}]}]})

(def (get G X) (exec #G @[(-field X)]))

(def test1 #(get g test1))
(== #test1 1)

(def test2 #(get g test2))
(== #(get test2 test21) 2)

(def test22 #(get test2 test22))
(== #(get test22 test3) 3)
