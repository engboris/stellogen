(use-macros "milkyway/prelude.sg")

(spec direction {
  [-north ok]
  [-south ok]
  [-west ok]
  [-east ok]})

(def n +north) (:: n direction)

(spec result { [(-ok X) ok] [(-error X) ok]})

(def x (+ok a)) (:: x result)

'pattern matching
(def get_ok {
  [(-ok X) X]
  [(-error X) (+error X)]})

(show (exec #get_ok @#x))
